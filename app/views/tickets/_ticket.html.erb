<!-- _________________________Create Ticket________________________________  -->

<div type="button" class="blog-main box text-center" id="ticket_<%= ticket.id %>">
  <div class="panel panel-info">
    <div>
      <!-- <%= ticket.score %> -->
      <% if ticket.score > 0.3 %>
        <div style="background: lightgreen; width: 20px; height: 20px; border-radius: 40px;"></div>
      <% end %>
      <% if ticket.score >= 0 && ticket.score <= 0.2  %>
        <div style="background: blue; width: 20px; height: 20px; border-radius: 40px;"></div>
      <% end %>
      <% if ticket.score < 0  %>
        <div style="background: red; width: 20px; height: 20px; border-radius: 40px;"></div>
      <% end %>
      <p><%= ticket.name %></p>
      <p><%= ticket.email %></p>
      <p><%= ticket.subject %></p>
      <%= link_to 'Open ticket', ticket_path(ticket) %>
    </div>
  </div>
</div>
